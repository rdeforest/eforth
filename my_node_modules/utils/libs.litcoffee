    module.exports = (opts) ->
      log = opts.utils.installLogger

      Object.assign {},
        ( for modName, exportName of opts.libs
            if exportName
              mod = require modName

              if 'string' is typeof exportName
                log "adding lib #{modName} as #{exportName}"
                exports[exportName] = mod
                undefined
              else
                log "adding lib #{modName}"
                (m = {})[modName] = mod
                m
        )...
